{
  "name": "back_api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build --type-check",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start ",
    "dev": "pnpm lint && cross-env NODE_ENV=development nest start  --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "npx prisma generate --schema src/prisma/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --schema src/prisma/schema.prisma",
    "prisma:push": "npx prisma db push --schema src/prisma/schema.prisma",
    "prisma:seed": "npx prisma db seed",
    "prisma:pull": "npx prisma db pull --schema src/prisma/schema.prisma",
    "prisma:format": "npx prisma format --schema src/prisma/schema.prisma",
    "prisma:studio": "npx prisma studio --schema src/prisma/schema.prisma"
  },
  "prisma": {
    "seed": "ts-node src/prisma/seed.ts"
  },
  "dependencies": {
    "@alicloud/dysmsapi20170525": "^3.1.1",
