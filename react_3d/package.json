{
  "name": "react_3d",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open --host",
    "build": "tsc && vite build --mode production",
    "build:test": "tsc && vite build --mode test",
    "build:docker": "tsc && vite build --mode docker",
    "preview": "vite preview",
    "lint": "tsc && eslint . --ext .js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write \"./**/*.{html,tsx,ts,js,json,md}\"",
    "prepare": "husky",
    "docker:build": "tsc && docker build -t docker-react .",
    "docker:run": "docker rm -f docker-react && docker run --name docker-react -d -p 80:8082 docker-react",
    "release:local": "pnpm docker:build && pnpm docker:run",
    "log:tree": "tree -N -I \"node_modules|.git|.idea\" -a"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "antd": "^5.22.1",
    "axios": "^1.7.7",
    "dayjs": "^1.11.13",
    "framer-motion": "^11.11.17",
    "i18next": "^23.16.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "rc-virtual-list": "^3.18.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.5",
    "react-i18next": "^15.1.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-router-dom": "^6.28.0",
    "recharts": "^2.15.0",
    "swiper": "^11.1.15",
    "three": "^0.170.0",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@types/node": "^20.17.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-transition-group": "^4.4.11",
    "@types/three": "^0.169.0",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-code-style-lint-react-ts": "^3.1.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.6",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "sass": "^1.81.0",
    "stylelint": "^16.10.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.2",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3",
    "unplugin-auto-import": "^0.17.8",
    "vite": "^5.4.11"
  }
}
